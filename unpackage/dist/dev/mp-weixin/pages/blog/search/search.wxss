.bg .search {
  display: flex;
  padding-top: 40rpx;
  height: 120rpx;
  background-color: white;
}
.bg .search input {
  width: 520rpx;
  height: 80rpx;
  padding-left: 80rpx;
  margin-left: 25rpx;
  margin-right: 30rpx;
  border-radius: 15rpx;
  background-color: #eeeeee;
}
.bg .search .sea {
  color: #b9bec2;
}
.bg .search uni-icons {
  position: absolute;
  top: 60rpx;
  left: 50rpx;
}
.bg .search .sear {
  width: 80rpx;
  height: 50rpx;
  color: #007afd;
  padding-top: 20rpx;
}
.bg .tab {
  width: 100%;
  height: 50rpx;
  padding-bottom: 20rpx;
  border-bottom: 1rpx solid #f0f0f0;
}
.bg .rec {
  display: flex;
  flex-wrap: wrap;
  align-content: flex-start;
  height: 980rpx;
}
.bg .rec .delete {
  width: 100%;
  height: 70rpx;
}
.bg .rec .delete .uniIcon {
  position: fixed;
  top: 250rpx;
  right: 20rpx;
}
.bg .rec .box {
  display: flex;
  margin-top: 80rpx;
}
.bg .rec .box .tag {
  font-size: 28rpx;
  height: 40rpx;
  color: #9fa4aa;
  padding: 15rpx;
  padding-left: 25rpx;
  padding-right: 25rpx;
  margin-top: -55rpx;
  margin-left: 20rpx;
  border-radius: 40rpx;
  background-color: #eeeeee;
}

